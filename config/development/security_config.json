{
    "refresh_cookie": {
        "enabled": true,
        "version": 1,
        "secure": false,
        "expiry": 57600000
    },
    "access_cookie": {
        "enabled": true,
        "version": 1,
        "secure": false,
        "expiry": 900000
    },
    "bearer": {
        "enabled": true,
        "header": "Authorization",
        "encryption": "aes-256-gcm",
        "expiry": 900000
    },
    "jwks": {
        "enabled": true,
        "cache": true,
        "cacheMaxEntries": 10,
        "cacheMaxAge": 3600000,
        "rateLimit": true,
        "timeout": 5000,
        "jwtksPerMinute": 5,
        "jwksUri": "https://demo.duendesoftware.com/.well-known/openid-configuration/jwks",
        "requestAgentCertPath": "",
        "issuer": "https://demo.duendesoftware.com",
        "audience": "api",
        "algorithms": [
            "RS256"
        ]
    },
    "swagger": {
        "enabled": true
    },
    "cors": {
        "origin": [
            "TODO: Block anything not in my shells (using a custom origin blocking middleware)"
        ],
        "allowedHeaders": [
            "Content-Type",
            "Authorization",
            "User-Agent",
            "X-Correlation-ID",
            "X-Swagger-API-Key"
        ],
        "methods": [
            "POST",
            "GET",
            "PATCH",
            "DELETE"
        ],
        "credentials": true,
        "maxAge": 600
    }
}