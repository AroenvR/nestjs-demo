{
    "cookie": {
        "enabled": true,
        "version": 1,
        "secure": false,
        "expiry": 3600,
        "maxAge": 57600000
    },
    "bearer": {
        "enabled": true,
        "header": "Authorization",
        "encryption": "aes-256-gcm",
        "expiry": 900
    },
    "swagger": {
        "enabled": true
    },
    "cors": {
        "origin": [
            "TODO: Block anything not in my shells (using a custom origin blocking middleware)"
        ],
        "allowedHeaders": [
            "Content-Type",
            "Authorization",
            "User-Agent",
            "X-Correlation-ID",
            "X-Swagger-API-Key"
        ],
        "methods": [
            "POST",
            "GET",
            "PATCH",
            "DELETE"
        ],
        "credentials": true,
        "maxAge": 600
    }
}