    "scripts": {
        "app": "npm run start:dev",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "test": "npm run lint && jest --maxWorkers=4",
        "test:watch": "npm run lint && jest --watchAll --maxWorkers=2",
        "test:coverage": "npm run lint && jest --coverage --maxWorkers=4",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"src/__tests__/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
    },
